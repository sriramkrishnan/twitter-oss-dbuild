build.cross-version: standard

build: {
  sbt-version: "0.13.8"
  projects: [
  {
    name:   chill
    system: sbt
    uri:    "git://github.com/sriramkrishnan/chill#java_cross_publish"
    check-missing: false
    extra: {
      run-tests: false
    }
  }, {
    name:   tormenta
    system: sbt
    uri:    "git://github.com/twitter/tormenta#develop"
    check-missing: false
    deps.ignore: "com.twitter#chill-java"
    extra: {
      run-tests: false
    }
  }
 ]
}
